{
    "extends": "@quasar/app-vite/tsconfig-preset",
    "compilerOptions": {
        "baseUrl": "./",
        "target": "es2016",
        // "module": "commonjs",
        "module": "es2020",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noUncheckedIndexedAccess": true,
        "noImplicitAny": true,
        "skipLibCheck": true,
        "resolveJsonModule": true
    }
}
